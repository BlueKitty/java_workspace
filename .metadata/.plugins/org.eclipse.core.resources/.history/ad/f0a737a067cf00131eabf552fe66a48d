package Parking_Ticket;

class ParkedCar {
	private String carMake, carModel, carColor, licenseNum;
	private int minsParked;
	
	public void setCarMake(String str) {
		carMake = str;
	}
	
	public String getCarMake() {
		return carMake;
	}
	
	public void setCarModel(String str) {
		carModel = str;
	}
	
	public String getCarModel() {
		return carModel;
	}
	
	public void setCarColor(String str) {
		carColor = str;
	}
	
	public String getCarColor() {
		return carColor;
	}
	
	public void setLicenseNum(String str) {
		licenseNum = str;
	}
	
	public String getLicenseNum() {
		return licenseNum;
	}
	
	public void setMinsParked(int i) {
		minsParked = i;
	}
	
	public int getMinsParked() {
		return minsParked;
	}
}

class ParkingMeter {
	private int timePurchased;
	
	public void setTimePurchased(int i) {
		timePurchased = i;
	}
	
	public int getTimePurchased() {
		return timePurchased;
	}
}

class ParkingTicket {
	private String make, model, color, licenseNum;
	private int fine;
	private String name, badge;
	
	public void setMake(String str) {
		make = str;
	}
	
	public String getMake() {
		return make;
	}
	
	public void setModel(String str) {
		model = str;
	}
	
	public String getModel() {
		return model;
	}
	
	public void setColor(String str) {
		color = str;
	}
	
	public String getColor() {
		return color;
	}
	
	public void setLicenseNum(String str) {
		licenseNum = str;
	}
	
	public String getLicenseNum() {
		return licenseNum;
	}
	
	public void setFine(int i) {
		fine = i;
	}
	
	public int getFine() {
		return fine;
	}
	
	public void setName(String str) {
		name = str;
	}
	
	public String getName() {
		return name;
	}
	
	public void setBadge(String str) {
		badge = str;
	}
	
	public String getBadge() {
		return badge;
	}
	
}